
@{
    ViewBag.Title = "Verification";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
@model IEnumerable<QualityControl.Db.Verification>

@section head{
    <style>
    .main-container {
        background-color: #FFFFFF;
        box-shadow: 0px 0px 2px 0px rgba(0, 0, 0, 0.05);
        margin: 0em auto;
        padding: 2em 2em 7em;
        z-index: 1;
    }

    .mycontent {
        margin-bottom: 30px;
    }
</style>}
<div class="main-container">

    <h2 class="ui dividing header">
        样品信息验证
        <a class="anchor" id="types"></a>
    </h2>

    <table class="ui  celled  table">
        <thead class="">
            <tr>
                <th>样品</th>
                <th>验证结果</th>
               
            </tr>
        </thead>
        <tbody>

            @if (Model.Count() == 0)
            {
            }
            else
            {
                foreach (var Tempx in Model)
                {
                    <tr>
                        <td>@Tempx.Num</td>
                        <td></td>
                    </tr>
                }
            }

        </tbody>
        <tfoot>
            <tr></tr>
        </tfoot>
    </table>
</div>
